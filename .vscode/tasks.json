{
  // VS Code Tasks to run generator and tests
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run oaw_to_rst.py",
      "type": "shell",
      "command": "python3 ${workspaceFolder}/oaw_to_rst.py",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated"
      },
      "problemMatcher": []
    },
    {
      "label": "Run run_test.py",
      "type": "shell",
      "command": "python3 ${workspaceFolder}/run_test.py",
      "group": "test",
      "presentation": {
        "reveal": "always",
        "panel": "dedicated"
      },
      "problemMatcher": []
    }
  ]
}

